<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdminController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="231.0" layoutY="14.0" prefHeight="26.0" prefWidth="179.0" style="&#10;" text="Admin Home" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS" wrapText="false">
          <font>
            <Font name="Arial Bold" size="29.0" />
          </font>
        </Label>
            <TextField fx:id="userInput" layoutX="366.0" layoutY="167.0" prefHeight="33.0" prefWidth="260.0" promptText="Enter username" />
            <Button fx:id="addButton" layoutX="557.0" layoutY="200.0" mnemonicParsing="false" text="Add User" />
            <ListView fx:id="userList" layoutX="19.0" layoutY="56.0" prefHeight="279.0" prefWidth="212.0" />
            <Button fx:id="deleteButton" layoutX="19.0" layoutY="335.0" mnemonicParsing="false" text="Delete" />
            <Button fx:id="logoutButton" layoutX="563.0" layoutY="348.0" mnemonicParsing="false" text="Logout" textFill="#c30000" />
      </children>
    </AnchorPane>
  </children>
</VBox>
